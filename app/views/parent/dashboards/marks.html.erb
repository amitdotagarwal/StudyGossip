
<th> Mark report</th>
<table class="table table stripped">
<tr>
<th>UserId</th>
<th>Class id</th>
<th>Subject id</th>
<th>min_marks</th>
<th> Maxmarks</th>
<th> Totalmarks</th>
<th> comment</th>
</tr>
<% @marks.each do |m| %>
<tr>
<td><%= m.user_id %></td>
<td><%= m.cls.class_name %></td>
<td><%= m.subject.subject_name %></td>
<td><%= m.min_marks %></td>
<td><%= m.max_marks %></td>
<td><%= m.total_marks %></td>
<td><%= m.comment %></td>

</tr>
<% end%>
</table>


