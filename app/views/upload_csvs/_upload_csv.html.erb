<%= form_for :user, :url => student_upload_csv_school_upload_csvs_path(@school), :html => {:multipart => true}, :remote => true do |f| %>
  <table>
    <tr>
      <td><%= f.label :csv, "Select CSV File" %></td>
      <td><%= f.file_field :csv %></td>
    </tr>
    <tr>
      <td><center><%= f.submit "Upload", :class => "btn btn-primary" %></center></td><br/>
    </tr>
  </table>
<% end %>
<span style="float: right;"><%#= link_to "Download CSV file",""  %></span>
